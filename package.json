{
    "name": "my-turborepo",
    "version": "0.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo build",
        "dev": "turbo dev",
        "lint": "turbo lint",
        "install:all": "yarn workspaces foreach --all install",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "@typescript-eslint/parser": "^6.1.0",
        "eslint": "^8.45.0",
        "prettier": "^3.0.0",
        "turbo": "^2.0.5",
        "typescript": "^5.1.6"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "packageManager": "yarn@4.3.1"
}
