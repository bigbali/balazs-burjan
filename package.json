{
  "name": "my-turborepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "prettier": "^2.8.3",
    "turbo": "^1.7.2",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@3.4.1"
}
